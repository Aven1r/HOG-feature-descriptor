@PACKAGE_INIT@

# Determine the platform library extension

if (TargetPlatform STREQUAL "Windows")
    set(hoglibrary_LIBRARIES "@CMAKE_INSTALL_PREFIX@/lib/hoglibrary.lib")
else()
    set(hoglibrary_LIBRARIES "@CMAKE_INSTALL_PREFIX@/lib/hoglibrary.a")
elseif()

# Additional variables or configurations specific to your library

# Provide imported target (optional)
add_library(hoglibrary INTERFACE)
target_include_directories(hoglibrary INTERFACE ${hoglibrary_INCLUDE_DIRS})
target_link_libraries(hoglibrary INTERFACE ${hoglibrary_LIBRARIES})